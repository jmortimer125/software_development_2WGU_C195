<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="c195.sw2.LogInController">
   <children>
      <Label fx:id="Title" layoutX="121.0" layoutY="14.0" text="Scheduling Software" textAlignment="CENTER">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
         <font>
            <Font size="40.0" />
         </font>
      </Label>
      <Label fx:id="zone" layoutX="49.0" layoutY="340.0" text="Label" />
      <TextField fx:id="enterUserName" alignment="CENTER" layoutX="220.0" layoutY="140.0" onAction="#enterUserName" promptText="Enter UserName">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </TextField>
      <TextField fx:id="enterPassword" alignment="CENTER" layoutX="220.0" layoutY="200.0" onAction="#enterPassword" promptText="Enter Password">
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </TextField>
      <Button fx:id="login" layoutX="323.0" layoutY="261.0" mnemonicParsing="false" onAction="#Login" text="Log In!" />
      <Button fx:id="Clear" layoutX="220.0" layoutY="261.0" mnemonicParsing="false" onAction="#clearLogin" text="Clear" />
      <Button fx:id="Exit" layoutX="531.0" layoutY="345.0" mnemonicParsing="false" onAction="#closeApp" text="Exit">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </Button>
      <Label fx:id="userName" alignment="CENTER" contentDisplay="CENTER" layoutX="261.0" layoutY="113.0" text="User Name" />
      <Label fx:id="password" alignment="CENTER" contentDisplay="CENTER" layoutX="265.0" layoutY="173.0" text="Password" textAlignment="CENTER">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </Label>
   </children>
</AnchorPane>
